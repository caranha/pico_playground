pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
max_shoots = 50
max_zombie = 500

shooter = {{x=20,y=40},
											{x=20,y=80}}

shoots = {}											
zombie = {}
			
function _update()
	foreach(shoots,update_shoot)
	foreach(zombie,update_zombie)
	
	if #shoots < max_shoots and rnd() > 0.7 then
		add(shoots,{x=20,y=shooter[flr(rnd(#shooter)+1)].y,dy=rnd() - 0.5})		
	end
	
	if #zombie < max_zombie and rnd() > 0.6 then
		add(zombie,{x=130,y=rnd(100)+20})
	end
	
	-- very bad --
	for s in all(shoots) do
		for z in all(zombie) do
			if collide(z,s) then
				del(zombie,z)
				del(shoots,s)
				break
			end
		end 
	end
end
								
function _draw() 
	cls()
	print("cpu: "..stat(1),0,0,7)
	print("units: "..#zombie+#shoots+#shooter,85,0,7)

	for i in all(shoots) do
		spr(2,i.x-4,i.y-4)
	end	
	for i in all(shooter) do
		spr(1,i.x-4,i.y-4)
	end
	for i in all(zombie) do
		spr(3,i.x-4,i.y-4)
	end
end

function update_zombie(z)
	z.x -= 0.15
	if (z.x < -10) del(zombie,z)
end

function update_shoot(s)
	s.x += 1
	s.y += s.dy
	if (s.x > 135) del(shoots,s)
end

function collide(z,s)
	return (abs(z.x - s.x) < 6 and 
	        abs(z.y - s.y) < 6)
end
