pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
cc = circfill

function _draw()
	cls(1)
	
	rectfill(0,100,128,128,3)
	
	for i = 1,7,1.5 do
		cc(20*i,85,7,6+i)
		cc(20*i,100,10,6+i)
		for j = 16,23,7 do
			ovalfill(j+20*(i-1),83,j+20*(i-1)+1,85,0)
		end
	end
end
