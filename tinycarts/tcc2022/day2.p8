function _draw()
	cls(1)
	
	for i = 1,100 do
		pset(i*27%128,(i*17+t()*30)%100,7)
	end

	rectfill(0,100,128,128,3)
	
	for i = 1,7,1.5 do
		cc(20*i,85,7,6+i)
		cc(20*i,100,10,6+i)
		cc(20*i-3,83,1,0)
		cc(20*i+3,83,1,0)
	end
end
