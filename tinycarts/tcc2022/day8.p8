version 38
__lua__
cls()
pal({8,8,8,8,8,9,9,9,9,10,10,10,10,8,9,10},1)
function _draw()
	⧗=t()

	for i=1,5 do
		a=⧗/2+i/5
		r=15+10*sin(⧗/2)
		s=6+sin(⧗/2)*3
		c=64+sin(⧗/5)*20
		circfill(80+cos(a)*r,
											c+sin(a)*r,
											s, 12)
	end

	circfill(108+rnd(20),rnd(128),rnd(2),11)	

	for x=0,128 do
	for y=0,128 do
		pset(x,y,pget(x+1,y)+1)
	end
	end
	
end
