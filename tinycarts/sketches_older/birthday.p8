pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t = 0
rockets = {}
explosion = {}

string = {"h","a","p","p","y"," ","b","i","r","t","h","d","a","y",","," ","t","r","a","s","e","v","o","l","!"}

function make_explosion(r)
	local nptc = 35+flr(rnd(15))
	for i = 1,nptc do
		local d = rnd()
		local e =  {
							x = r.x+cos(d),
							y = r.y+cos(d),
							d = d,
							v = 0.8+rnd(0.2),
							c = r.c+rnd(4)-2,
							a = 8+rnd(4)
						}
		if (flr(rnd(2))%2 == 0) e.v *= 1.5
		add(explosion,e)
	end
end

function update_explosion(e)
	e.x += cos(e.d)*e.v*2
	e.y += sin(e.d)*e.v*2
	e.v *= 0.9
	e.y += 0.6

	e.a -= 1
	if (e.a < 0) del(explosion,e)
end

function draw_explosion(e)
	pset(e.x,e.y,e.c)
end

function new_rocket()
	add(rockets,
					{x = 64, y = 140,
						d = sin(t/30)/8 + 0.25,
						c = rnd()*8 + 8 ,
						a = rnd()*10 + 20})
end

function update_rocket(r)
	r.x += cos(r.d)*3
	r.y += sin(r.d)*4.5
	r.a -= 1
	if r.a < 1 then
		del(rockets,r)
		make_explosion(r)
	end
end

function draw_rocket(r)
	circfill(r.x, r.y, 1.5, r.c)
	for i = 1,3 do
		pset(r.x-cos(r.d)*3*i, 
							r.y-sin(r.d)*4.5*i, 
							r.c)
	end
end

function draw_text()
	for i = 1,#string do
		for j = 1,5 do
			print(string[i],i*5,100+sin((t+j+i)/25)*10,7+j)
		end
	end
end

function _update()
	t += 1
	if (rnd() < 0.08) new_rocket()
	foreach(rockets, update_rocket)
	foreach(explosion, update_explosion)
end

function _draw()
	cls()
	foreach(rockets,draw_rocket)
	foreach(explosion,draw_explosion)
	draw_text()
end
