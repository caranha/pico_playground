pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _init()
 p = {x = 30, y = 0,
	 				sx = 2, sy = 0,
		 			boost = 0,
		 			state = 0, -- ground, jump, wall
		 			bb = {{1,0},{5,0},{1,7},{5,7}}
     }
 pixels = {}
    
 c = {x = 0, dx = 0}
 t = 0
 
 update_frame = 0
	next_update = 256
	last_h = 25
	 
 for y0 = 0,15 do
		memcpy(0x2000 + y0*128,
		       0x2000 + 64 + y0*128,
		       64)  
	end
end

function get_tile(x,y)
	return mget(flr(x/8)%64,flr(y/8))
end

function set_tile(x,y,tl)
	mset(flr(x/8)%64,flr(y/8),tl)
end

function destroy_tile(x,y)
	local c = get_tile(x,y)
	if (c == 0) return
	set_tile(x,y,0)
	for ox = 0,7 do
		for oy = 0,7 do
			local cc = sget((c*8)%128+ox,
			                flr(c/16)*8+oy)
			local chance = (200 - #pixels)/200
			if cc > 0 and rnd() < chance then
				local d = rnd(.4)-0.2
				local s = rnd(2)+2
				add(pixels,{x = x+ox, y = y+oy, dx = cos(d)*s, dy = sin(d)*s, c = cc, a = rnd(10)+5})
			end
		end
	end
end

function col_tile(e)
	for i = 1,4 do
		local f = fget(
													get_tile(
														e.x+e.bb[i][1],
														e.y+e.bb[i][2]
													),
		           0)
		if (f) return true
	end
	return false	
end



function _update60()
	t += 1
	player_update()
	if p.x > next_update then
		map_update((flr(p.x/128) + 2) % 4)
		next_update += 128
	 update_frame = (update_frame+1)%4
	end
	
	crawler_update()
	foreach(pixels,pixels_update)
	
end

function map_update(dest)
	if update_frame == 0 then
		map_gen()
	end
	for y0 = 0,15 do
		memcpy(0x2000 + dest*16 + y0*128,
		       0x2000 + update_frame*16 + (y0+16)*128,
		       16)  
	end
end

function map_create_column(xpos,ypos,length)
	local btm = (length and length or 5+flr(rnd(3)))
	local ly = ypos+1
	while ly < min(31,ypos+btm) do
		tile = (rnd() < .9 and 24 or 25 + flr(rnd(3))) 
		mset(xpos,ly,tile)
		ly += 1
	end
	mset(xpos,ly,40+flr(rnd(2)))
end

function map_gen()
	for y = 0,15 do
		memset(0x2000 + (y+16)*128, 0, 16*4)
	end
	
	map_gen_hills()
end




-- x 00-63, y 16-31
function map_gen_hills()
	local ramp = rnd(4)
	local rfrq = rnd(.5)+.1

	for x = 0,63 do
			local y = min(28,max(18,last_h+sin((x*rfrq)/16)*ramp))
			local tile = (rnd() < .6 and 8 or 9 + flr(rnd(3)))
			mset(x,y,tile)
			if (x == 63) last_h = y
			map_create_column(x,y,15)
	end
end



function crawler_update()
	c.dx = min(1, c.dx + .01)
	c.x += c.dx
	if p.x - c.x < 90 then
		for i = 1,5 do
			destroy_tile(c.x+rnd(10),rnd(128))
 	end
 else
 	c.x += c.dx
 end
end

function player_update()

	p.sy = min(p.sy+0.4,2)
	
	if btnp(4) then
		if p.state == 0 then
			p.sy = -4 p.state = 1
		elseif p.state == 2 then
		 p.sy = -4 p.sx *= -1 p.state = 1
		end		 
	end
		
	-- x move
	local ox = p.x
	local t = 1
	p.x = ox + t*p.sx	
	while col_tile(p) and t > 0.1 do
		t /= 2
		p.x = ox + t*p.sx
	end	
	if col_tile(p) then		
		p.x = ox
		if (p.state == 1) p.state = 2
	else
		if (p.state == 2) p.state = 1
	end
	
	
	-- y move
	local oy = p.y
	p.y += p.sy
	if col_tile(p) then
		p.y = flr(oy)
		if p.sy > 0 then
			p.state = 0
			p.sx = abs(p.sx)
		end		
		p.sy = 0
	else 
		if (p.sy > 1 and p.state != 2) p.state = 1
	end
	
	-- dead
	if (p.y > 128 or p.x < c.x) _init()
end

function pixels_update(pp)
	pp.a -= 1
	pp.x += pp.dx
	pp.y += pp.dy
	if (pp.a < 1) del(pixels,pp)
end


function _draw()
	cls()
	
	camera(max(c.x,p.x-80))	
	map_draw()
	player_draw()
	for pp in all(pixels) do
		pset(pp.x,pp.y,pp.c)
	end
	
	camera()
	print(stat(1).." "..#pixels,0,0)
end

function map_draw()
	local sec = flr(p.x/128)
	map((sec-1)%4*16,0, (sec-1) * 128,0,16,16)
	map(sec%4*16,0,sec * 128,0,16,16)
	map((sec+1)%4*16,0, (sec+1) * 128,0,16,16)
end

function player_draw()
 spr(1 + flr(t/3)%2,p.x,p.y)
end
