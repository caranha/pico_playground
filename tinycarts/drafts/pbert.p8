pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
state = 0

function _update()
	update_c()
end

function _draw()
	local bck = (state==0 and 0 or rnd(16)+1) 
	cls(bck)
	drawcubes()
	draw_c()
	
	print(c_row,0,0,7)
	print(c_col,0,8,7)
	print(calc_ind(c_row,c_col),0,16,7)
	
end
