pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
boids = {}

goal = {x=1,y=1}

function _init()
	for i = 1,10 do
		add(boids,{x=rnd(20),y=rnd(20),d=rnd(),c=rnd(5)+4})
	end	
end

function _update()
	mean_dx = 0
	mean_dy = 0
	mean_px = 0
	mean_py = 0
	
	for b in all(boids) do
		mean_dx += cos(b.d) /#boids
		mean_dy += sin(b.d) /#boids
		mean_px += b.x /#boids
		mean_py += b.y /#boids
	end
	
	for b in all(boids) do
		mean_d = atan2(mean_dx,mean_dy)
		goal_t = atan2(goal.x,goal.y)
		mean_p = atan2(b.x - mean_px,b.y - mean_py)

	
		dd = sgn(b.d - mean_d) * 0.05
		if (abs(b.d - mean_d) > 0.5) dd *= -1
		dt = sgn(b.d - goal_t) * 0.05
		if (abs(b.d - goal_t) > 0.5) dt *= -1
		dp = sgn(b.d - mean_p) * 0.05
		if (abs(b.d - mean_p) > 0.5) dp *= -1 
		
		b.d += dt
		b.d += dd
		b.d += dp
		b.x += cos(b.d)
		b.y += sin(b.d)

	end
end

function _draw()
	cls()
	camera(boids[1].x-64,boids[1].y-64)
	for b in all(boids) do
		line(b.x,b.y,b.x+cos(b.d)*3,b.y+sin(b.d)*3)
		circ(b.x,b.y,1.5,b.c)
	end
end
