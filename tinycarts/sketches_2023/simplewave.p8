version 38
__lua__
function _draw()
	cls()
	for x=1,128,2 do
	for y=1,128,4 do
		d = ((x-64)^2 + (y-64)^2)^0.5
		w = sin(d/20-t())
		if w < 0 then
			pset(x,y+w*5,5+(w*2)%3)
		else
			pset(x,y,1)
		end
	end
	end
end
