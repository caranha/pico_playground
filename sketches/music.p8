pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- music cart
-- by caranha

-- songs
-- 0 - bass line
-- 4 - repeat
-- 5 - musik fly

local play = false
local select = 0
local mstart = {0,4,5,11,17}
local t = 0

local notes = {}

function _update()
	t += 1
	
	if play then
		if #notes < 10 and rnd() < 0.05 then
			add(notes,{x = 98, y = 98,
														a = 8, ma = 20+rnd(10),
														d = 0.05+rnd(0.4), 
														err = rnd(0.05),
														c = 8 + flr(rnd(5))})
		end
	end

	for n in all(notes) do
		n.a += 0.2
		if (n.a > n.ma) del(notes,n)
	end
	
	if btnp(2) then sfx(10) select = (select-1)%#mstart end
	if btnp(3) then sfx(10) select = (select+1)%#mstart end
	
	if btnp(5) then
		play = true
		sfx(11)
		music(mstart[select+1])
	end
	if btnp(4) then
		play = false
		music(-1)
	end
end

function _draw()
	cls()

 print(">", 3+t/30%2, 20+select*8)
	
	color(2)
	print("song 1 - bass line", 9,21)
	print("song 2 - repeat", 9,29)
	print("song 3 - music fly", 9,37)
	print("song 4 - incompetent solo", 9,45)
	print("song 5 - penta-cool", 9,53)
	if play then
		print("press é to stop",9,66)
	else
		print("press ó to play",	9, 66)
	end

	color(6)
	print("song 1 - bass line",10,20)
	print("song 2 - repeat",10,28)
	print("song 3 - music fly",10,36)
	print("song 4 - incompetent solo", 10,44)
	print("song 5 - penta-cool", 10,52)
	if play then
		print("press é to stop",10,65)
	else
		print("press ó to play",	10,65)
	end
	
	if (play) pal(8,8+flr(rnd(3)))
	spr(2,90,90,2,4)
	
	pal()
	
	for n in all(notes) do
		print("ç",
								n.x+cos(n.d)*n.a,
								n.y+sin(n.d)*n.a,
								n.c)
	end
end
