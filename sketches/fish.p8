pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
shake = 0
t = 0
c = {x = 0, y = 0}

fish = {}
bbl = {}
splsh = {}
plant = {}
stars = {}

for i = 1,50 do
	add(stars,{rnd()*128,rnd()*70,true})
end

function draw_plant(p)
	for i = 1,#(p.part) do
		spr(p.part[i][1] + 32,
						p.x, p.y - 8*(i-1),
						1, 1, p.part[i][2]) 
	end
end

function new_plant(x)
	p = {}
	p.x = x
	p.y = 93 + rnd()*3
	part = {}
	size = flr(rnd()*3)+2
	for i = 1,size do
		add(part,{flr(rnd()*4),
												(rnd() > 0.5)})
	end
	p.part = part
	add(plant,p)
end


for i = 1,20 do
	new_plant(rnd()*300 - 150)
end

add(fish, {x = 0, y = 30,
											spd = 3, d = 0,
											ow = 0, id = 0})
add(fish, {x = 0, y = 30,
											spd = 3, d = 0,
											ow = 0, id = 1})
add(fish, {x = 0, y = 30,
											spd = 3, d = 0,
											ow = 0, id = 2})
add(fish, {x = 0, y = 30,
											spd = 3, d = 0,
											ow = 0, id = 3})

function update_splsh(s)
	s.y += sin(s.a / 15) * abs(s.spd*1.5)
	s.x += s.spd
	s.a += 1
end

function update_bbl(b)
	b.y -= 0.2
	b.x += (rnd(1)-0.5)*0.3
	b.s += 0.1
end

function update_fish(f)
	f.x += f.spd*cos(f.d)
	f.y += f.spd*sin(f.d)
	rd = rnd(1)
	if (rd < 0.11) f.d -= 0.1
	if (rd > 0.99) f.d += 0.1
	if (f.d < 0) f.d += 1
	if (f.d >= 1) f.d -= 1
	
	if rnd(1) > 0.9 then
		add(bbl,{x = f.x, y = f.y, s = 1})
	end

	if f.y < 0 then
		f.ow += 1
	else
		if (f.ow > 0) then
			add(splsh,{x = f.x,y = 0,
														a = 0, spd = 1.5,
														s = 1})
			add(splsh,{x = f.x,y = 0,
														a = 0, spd = -1.5,
														s = 1})
			sfx(0)
			if (f.id == 0) shake += f.ow
		end
		f.ow = 0
	end
		
	f.y += 0.1 * (f.ow) 	
	if (f.y > 99) f.y = 99
	
	if f.id > 0 and abs(f.x - fish[1].x) + abs(f.y - fish[1].y) > 70
	then
				fzd = atan2(fish[1].x - f.x, fish[1].y - f.y)
				if abs(f.d-fzd) > 0.5 
				then
				 f.d += 0.1*sgn(f.d-fzd)
				else 
					f.d -= 0.1*sgn(f.d-fzd)
				end
	end	
end

function _update()
	t += 0.2
	if (t > 10000) t = 0
	if (shake > 0) shake -= 1
	
	for f in all(fish) do
		update_fish(f)
	end
	for b in all(bbl) do
		update_bbl(b)
		if (b.s > 4 or b.y < 0) del(bbl,b)
	end
	for s in all(splsh) do
		update_splsh(s)
		if (s.a > 15) del(splsh,s)
	end
 	
	c.x = (c.x + fish[1].x)/2
	c.y = (c.y + fish[1].y)/2	
	c.y = max(c.y, -10)
	c.y = min(c.y, 70)
	
	for p in all(plant) do
		if (abs(c.x - p.x) > 170) del(plant,p)
	end
	if #plant < 30 then
		for i = 1,(30 - #plant) do
		x = rnd()*50
		d = rnd() - 0.5
		new_plant(c.x + sgn(d)*(64 + x))
		end
	end
end

function _draw()
	cls()
	sshake = min(shake,12)
	camera(0 + rnd(sshake),
								(c.y-64)/20 + rnd(sshake))
	for s in all(stars) do
		circfill(s[1],s[2],0,
											(s[3] and 6 or 13))
	 if (rnd() > 0.99) s[3] = not(s[3])
	end
	circfill(20,10,15,7)
	
	camera(c.x-64 + rnd(sshake), 
								c.y-64 + rnd(sshake))
	
	rectfill(c.x-150,2,c.x+150,100,12)
	rectfill(c.x-150,100,c.x+200,103,9)
	rectfill(c.x-150,104,c.x+200,150,4)	

	for w = -70,70 do
		line(c.x + w, 2, c.x + w, 1 - sin((c.x+w)/30)*1.5, 12)
	end


	for b in all(bbl) do
		circ(b.x,b.y,b.s,1)
	end
	for s in all(splsh) do
		circfill(s.x,s.y,s.s,12)
	end
	for f in all(fish) do	
		spr(1 + flr(t%2) + 
						2*flr((f.d*4)%4),
						f.x,f.y)
	end
	for p in all(plant) do
		draw_plant(p)
	end
end


