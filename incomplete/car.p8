pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
bcd = 0
cart = {}
blt = {}
fx = {}

cart[1] = {x = 80, y = 230, 
           s = 0, ds = 0.1, ms = 2,
           d = 0.25, dd = 0.01,
           sp = 1, sc = 3, im = true}
           
for i = 1,10 do
	add(cart,
	    {x = rnd(120)+30, y = rnd(120)+30,
	     s = 0, ds = 0.1, ms = 0.5+rnd(2),
	     d = rnd(), dd = 0.01*sgn(rnd()-0.5), 
	     sp = 1, sc = rnd(16)})
end


function npc_ctl(c)
	if (c.im) return
 c.d -= c.dd
 c.s = min(c.ms,c.s+c.ds)
	c.x += cos(c.d)*c.s
	c.y += sin(c.d)*c.s
	local t = false
	for i=1,#cart do
		if cart[i] != c and abs(cart[i].x - c.x) < 5 and abs(cart[i].y - c.y) < 5 then
			c.s = -c.s
		end
	end
end

function p_ctl(c)
	
	if (btn(1)) c.d -= c.dd
	if (btn(0)) c.d += c.dd
	if (btn(2)) c.s = min(c.ms,c.s+c.ds)
	if (btn(3)) c.s = max(-c.ms/2,c.s-c.ds)
	
	if (btn(4) and bcd < 1) then
	 add(blt,{x = c.x,
	          y = c.y,
	          d = c.d,
	          t = 0})
	 bcd += 3
	 c.x -= cos(c.d)*0.3
	 c.y -= sin(c.d)*0.3
	 sfx(0)
	end
	bcd = max(0, bcd -1)
	
	for i=1,#cart do
			if cart[i] != c and abs(cart[i].x - c.x) < 11 and abs(cart[i].y - c.y) < 11 then
			c.s = -c.s
		end
	end
	
	c.x += cos(c.d)*c.s
	c.y += sin(c.d)*c.s

end

function update_blt()
	for b in all(blt) do
		b.x += cos(b.d)*2
		b.y += sin(b.d)*2
		b.t += 1
		for c in all(cart) do
			if not c.im and abs(c.x - b.x) < 4 and abs(c.y - b.y) < 4 then
			 del(cart,c)
			 for i = 1,10 do
			  add(fx,{x = b.x+rnd(5),y = b.y+rnd(5), s = rnd(3), c = 8+rnd(3), t = rnd(6)+1})
			 end
			 sfx(1)
			 b.t += 60
			end
		end
		if (b.t > 60) del(blt,b)
	end
end

function update_fx()
	for f in all(fx) do
		f.t -= 1
		if (f.t < 0) del(fx,f)
	end
end

function _update()
	p_ctl(cart[1])
	foreach(cart,npc_ctl)
	update_blt()
	update_fx()
end

function _draw()
	cls(4)
	
	local cx = max(0,min(128,cart[1].x - 64))
	local cy = max(0,min(128,cart[1].y - 64))
	camera(cx,cy)
	
	map(0,0)	
	print(stat(1))
	
	-- camera(p1.x-64,p1.y-64)
	foreach(cart,draw_car)
	foreach(blt,draw_blt)
	foreach(fx,draw_fx)
end

function draw_blt(b)
	pset(b.x,b.y,10)
end

function draw_car(c)
	pal(6,c.sc)
	local sd = 0
	if (abs(cos(c.d)) < 0.95) sd = 1
	if (abs(cos(c.d)) < 0.8) sd = 2
	if (abs(cos(c.d)) < 0.58) sd = 3
	if (abs(cos(c.d)) < 0.3) sd = 4

	spr(8-sd*2,c.x-8,c.y-8,2,2,(c.d%1 < 0.75 and c.d%1 > 0.25),c.d%1 > 0.5)
	--line(c.x,c.y,c.x + 5*cos(c.d), c.y + 5*sin(c.d),7)
	pal()
end

function draw_fx(f)
	circfill(f.x, f.y, f.s, f.c)
end

