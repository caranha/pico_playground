pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

t = 0

fallspeed = 4
fallspeedvar = 2

windspeed = 1.5
offset = 0

ndrops = 50

drops = {}
splash = {}

function movedrop(d)
    d.x += windspeed
    d.y += d.speed
end

function drawdrop(d)
    rectfill(d.x,d.y,d.x+1,d.y+1,d.color)
    rectfill(d.x+offset,d.y+1,d.x+offset+1,d.y+1+1,d.color)
end

function drawsplash(d)
    rectfill(s.x-1,s.y-1,s.x,s.y,s.color)
    rectfill(s.x+1,s.y-1,s.x+2,s.y,s.color)
end

function newdrop()
    d = {x = rnd(127+abs(windspeed*127)), y = 0, color = 1}
    if (windspeed > 0) then d.x -= windspeed*127 end
    d.speed = fallspeed + rnd(fallspeedvar)
    add(drops,d)
end

function newsplash(d)
    s = {x = d.x, y = d.y, age = 1, color = d.color}
    add(splash,s)
end
    
function _init()
end

function _update60()
    t += 1
    if t > 240 then
        windspeed = rnd(3) - 1.5
        t = 0
    end
    for d in all(drops) do
        movedrop(d)
        if (d.y > 127 or pget(d.x,d.y) != 0) then
            del(drops,d)
            newsplash(d)
        end
    end
    for s in all(splash) do
        s.age -= 1
        if s.age < 0 then del(splash,s) end
    end
    if #drops < ndrops then
        newdrop()
    end
end

function _draw()
    cls()
    rectfill(30,100,70,110,3)
    rectfill(60,50,120,65,3)
    foreach(drops,drawdrop)
    foreach(splash,drawsplash)
end

