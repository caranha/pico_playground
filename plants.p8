pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
selector = {}
mov = {{-1,0},{1,0},{0,-1},{0,1}}

zombiecd = 20

plants = {}
bullets = {}
zombies = {}

function spawnzombie()
	zombiecd -= 1
	if zombiecd < 0 then
		add(zombies,
				  {x = 130,
				   y = rnd(80)+20,
				   spd = 1,
				   cd = 2
				  })
	end
end

function updatezombie()
	for i in all(zombies) do
		if i.x < 0 then
			del(zombies,i)			
		end
		if i.cd < 0 then
			i.x -= i.spd
			i.cd = 2
		end
		i.cd -= 1
		for j in all(plants) do
			if colsprite(i,j) then
				del(plants,j)
			end
		end
	end
end

function colsprite(i,j)
	if i.x < j.x+8 and
	   i.x+8 > j.x and
	   i.y < j.y+8 and
	   i.y+8 > j.y then
	   return true
 end	
end

function updatebullets()
	for i in all(bullets) do
		i.x += 1
		if (i.x > 130) then
			del(bullets,i)
		end
		for j in all(zombies) do
			if colsprite(i,j)  then
				del(bullets,i)
				del(zombies,j)
			end
		end		
	end
end

function updateplants()
	for i in all(plants) do
		if i.t == 1 and i.cd < 0 then
			add(bullets,{x = i.x,y = i.y})
			i.cd = 10
		end
		if i.t == 2 and i.cd < 0 then
		 i.cd = 20 
		 selector.money += 50
		end
		i.cd -= 1
	end
end

function newplant(x,y,t)
	p = {}
	p.x = x
	p.y = y
	p.t = t
	p.cd = 10
	add(plants,p)
end

function control()
 for x=0,3 do
 	if btn(x) == true then
 		selector.x += mov[x+1][1]
 		selector.y += mov[x+1][2]
 	end
 end
 if btn(4) and selector.cd < 0 
 then
 	selector.plant += 1
 	selector.plant = selector.plant % 3
 	selector.cd = 5
 end
 if btn(5) and 
 			selector.plant > 0 and
 			selector.cd < 0 
 then
 	newplant(selector.x,selector.y,selector.plant)
 	selector.cd = 5
 end
 
 selector.cd -= 1
end

function _init()
 selector.x = 20
 selector.y = 20
 selector.plant = 1
 selector.cd = 5
 selector.money = 0
end

function _update()
	control()
	updateplants()
	updatebullets()
	spawnzombie()
	updatezombie()
end

function drawplant(p)
	spr(2+p.t*16,p.x,p.y)
end

function drawbullet(p)
	spr(5+16,p.x,p.y)
end

function drawzombie(z)
	spr(24,z.x,z.y)
end

function _draw()
	cls(15)
	--spr(18,50,50)
	
	foreach(plants,drawplant)
	foreach(bullets,drawbullet)
	foreach(zombies,drawzombie)
	print("money: "..selector.money,80,0,5)
	
	spr(2+selector.plant*16,selector.x,selector.y)
 spr(1,selector.x,selector.y)
end
